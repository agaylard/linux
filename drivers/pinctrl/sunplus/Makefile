obj-$(CONFIG_PINCTRL_SPPCTL) += sppctl_gpio_ops.o
obj-$(CONFIG_PINCTRL_SPPCTL) += sppctl_gpio.o
obj-$(CONFIG_PINCTRL_SPPCTL) += pinctrl_inf_sp7021.o
obj-$(CONFIG_PINCTRL_SPPCTL) += gpio_inf_sp7021.o

obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += sppctl.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += sppctl_pinctrl.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += sppctl_sysfs.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += sppctl_gpio_ops.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += sppctl_gpio.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += pinctrl_inf_q645.o
obj-$(CONFIG_PINCTRL_SPPCTL_Q645) += gpio_inf_q645.o
